{
  "expo": {
    "name": "parco-montreal",
    "slug": "parco-montreal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-mtl.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-mtl.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.grimmjowx9.parcomontreal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bc1349ce-5c76-4aa0-b1c4-11b102024f3e"
      }
    }
  }
}
